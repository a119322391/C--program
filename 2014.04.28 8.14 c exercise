// phone.cpp : 定義主控台應用程式的進入點。
//

#include "stdafx.h"
#include <stdio.h>
#define SIZE 80
//int _tmain(int argc, _TCHAR* argv[])
//{
	//return 0;
//}

int main(void){
	int i = 0;
	int c;
	char *tokenPtr;
	char string;
	char sentence[SIZE];
	while (i < SIZE - 1 && (c = getchar()) != '\n'){
		sentence[i++] = c;
	}

	sentence[i] = '\0';
	tokenPtr = strtok(string, "");
	while (tokenPtr != NULL){
		printf("%s", tokenPtr);
		tokenPtr = strtok(NULL, " ");
	}
}
