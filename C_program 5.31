#include "stdafx.h"
#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 3


int main(void){
	size_t face;
	unsigned int roll;
	unsighned frequency[SIZE] = {0};

	srand (time(NULL));

	for(roll = 1; roll <= 100;++roll){
		face = 1 + rand() % 2;
		++frequency[face];
	}
	printf("%s%17s\n","Face","Frequency");

	for(face = 1; face < SIZE;++face){
		printf("%s%17s\n",face,frequency[face]);
	}
}
